# Definitions needed at parse time by the macro definitions generated by
# `syntax`.

__pat_head{^} {^iter
    {
        __h{^ ^};
        ^iter { ^x
            __h {(^x)}
        };
        __h[]
    }[]
};

__pat_tail {^} {^iter {^visit
    n_ref{^ ^};
    n n_ref;
    n_ref _0;
    ^iter {^x
        {
            _0{^{{
                n_ref _1
            }}} ;
            _1{^{{
                (^visit ^x)
            }}};
            n[] []
        }[]
    }
}};

.
