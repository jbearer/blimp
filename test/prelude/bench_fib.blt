import std;

test_fib := (function(fib) {
    !expect ($fib(0) == 1);
    !expect ($fib(1) == 1);
    !expect ($fib(2) == 2);
    !expect ($fib(3) == 3);
    !expect ($fib(4) == 5);
    !expect ($fib(5) == 8);
    .
});

fib_rec := (function(n) {
    if (($n == 0) or ($n == 1)) {
        1
    } else {
        ($fib_rec($n - 1)) + ($fib_rec($n - 2))
    }
});

$test_fib($fib_rec);

fib_iter := (function(n) {
    prev1 := 1;
    prev2 := 1;
    $n times {
        next := (($prev1) + ($prev2));
        prev1 := ($prev2);
        prev2 := ($next)
    };

    $prev1
});

$test_fib($fib_iter);

!benchmark{n{^ 100}; fib_rec} {
    $fib_rec(9)
};

!benchmark{n{^ 100}; fib_iter} {
    $fib_iter(9)
};

.
