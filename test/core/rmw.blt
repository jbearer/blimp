bind . _ .;

# Check that the result of %= is the parent object of the receiver.
bind symbol check (
    this{%=|{.|.}} {!expect|x};
        # The parent of the `this` object is `x`.
    this{%=|{.|.}}{%=|{.|.}} {!expect|y}
        # The parent of `x` is `y`.
);
bind symbol do_test (
    x{check|.}
);
y{do_test|.}
