#: --skip-racket -fno-gc-refcount -fno-gc-cycle-detection

# Tests garbage collection of 10 element simple cycles (C10).

import std;

node{^
    {
        {^cell
            {^next
                ^cell {(^next)}
            }
        }(__cell[])
    }
};

:benchmark{n{^ 5000}; ops{^ 10}; tracing_cycle} {
    __push_stack_frame[];

    # Create 10 nodes.
    n1 := (node[]);
    n2 := (node[]);
    n3 := (node[]);
    n4 := (node[]);
    n5 := (node[]);
    n6 := (node[]);
    n7 := (node[]);
    n8 := (node[]);
    n9 := (node[]);
    n10 := (node[]);

    # Link them into a cycle.
    ($n1) ($n2);
    ($n2) ($n3);
    ($n3) ($n4);
    ($n4) ($n5);
    ($n5) ($n6);
    ($n6) ($n7);
    ($n7) ($n8);
    ($n8) ($n9);
    ($n9) ($n10);
    ($n10) ($n1);

    # Leak the nodes.
    n1 := nil;
    n2 := nil;
    n3 := nil;
    n4 := nil;
    n5 := nil;
    n6 := nil;
    n7 := nil;
    n8 := nil;
    n9 := nil;
    n10 := nil;

    __pop_stack_frame[];

    .
};

:gc_collect;
:gc_print_stats;
:gc_check_collect;
.
